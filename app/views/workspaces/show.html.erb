<h1 id="ratione"> <%= @workspace.name %> </h1>

<p>Location: <%= @workspace.location %></p>



<ul>
    <% @workspace.users.each do |user| %>
        <li>
            <%= link_to user.name, user %> - <%= user.title %> - <%= user.user_rating %>
        </li>
    <% end %>

    
 

<% if @current_user.title == "Boss" %>
<%= link_to "Edit", edit_workspace_path(@workspace)%>
<% end %>


        

